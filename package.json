    "engines": {
        "node": "18.x"
    },
    "scripts": {
        "build": "nest build",
        "build:all": "node ./scripts/build-all-service.js",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,types}/**/*.ts\" --fix",
        "start": "nest start",
        "repl": "nest start api --entryFile ../../repl.ts",
        "repl:dev": "nest start api --watch --entryFile ../../repl.ts",
        "seeder": "nest start api --entryFile ../../seeder.ts",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node ./scripts/start-production.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./apps/back-end/test/jest-e2e.json",
        "compodoc": "node ./scripts/run-compodoc.js",
        "quick:start": "./scripts/quick-start-local.ps1",
        "quick:dev": "./scripts/quick-dev-local.ps1",
        "get-logs": "docker cp swag:/config/log/. ./logs && docker cp utility:/usr/src/app/logs .",
        "generate:swagger-yaml": "node ./scripts/swagger-json-to-yaml.js",
        "generate:asyncapi-yaml": "node ./scripts/asyncapi-json-to-yaml.js",
        "generate:sdk": "npx @openapitools/openapi-generator-cli generate",
        "env": "node ./scripts/convert-base64.js",
        "compose": "node ./scripts/docker-compose-helper.js"
    },
