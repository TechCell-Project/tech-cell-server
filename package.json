{
  "name": "tech-cell-web-server",
  "version": "0.0.1",
  "description": "",
  "author": "TechCell Teams <teams@techcell.cloud>",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "nest build",
    "build:all": "node ./scripts/build-all-service.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node ./scripts/start-production.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/back-end/test/jest-e2e.json",
    "up:core": "docker compose -f docker-compose.core.yml up -d",
    "down:core": "docker compose -f docker-compose.core.yml down",
    "restart:core": "yarn down:core && yarn up:core --build",
    "up": "docker compose -f docker-compose.yml up -d",
    "up:build": "docker compose build --no-cache && docker compose -f docker-compose.yml up -d --force-recreate",
    "down": "docker compose -f docker-compose.yml down",
    "restart": "yarn down && yarn up:build",
    "up:all": "yarn up:core && yarn up",
    "down:all": "yarn down:core && yarn down",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.0.3",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/swagger": "^7.0.4",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.1.0",
    "amqp-connection-manager": "^4.1.13",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.1",
    "morgan": "^1.10.0",
    "nestjs-throttler-storage-redis": "^0.3.3",
    "nodemailer": "^6.9.3",
    "otp-generator": "^4.0.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/nodemailer": "^6.4.8",
    "@types/otp-generator": "^4.0.0",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^~/constants(|/.*)$": "<rootDir>/constants/$1",
      "^~/apps/api(|/.*)$": "<rootDir>/apps/api/src/$1",
      "^~/apps/auth(|/.*)$": "<rootDir>/apps/auth/src/$1",
      "^~/apps/mail(|/.*)$": "<rootDir>/apps/mail/src/$1",
      "^~/apps/products(|/.*)$": "<rootDir>/apps/products/src/$1",
      "^~/apps/sample(|/.*)$": "<rootDir>/apps/sample/src/$1",
      "^~/apps/managaments(|/.*)$": "<rootDir>/apps/managaments/src/$1",
      "^@app/resource(|/.*)$": "<rootDir>/libs/resource/src/$1"
    }
  }
}
