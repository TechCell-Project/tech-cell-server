        "@typescript-eslint/parser": "^5.60.0",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "29.5.0",
        "prettier": "^2.8.8",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/apps/",
            "<rootDir>/libs/"
        ],
        "moduleNameMapper": {
            "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
            "^@app/resource(|/.*)$": "<rootDir>/libs/resource/src/$1",
            "^@app/third-party(|/.*)$": "<rootDir>/libs/third-party/src/$1",
            "^~/apps/api(|/.*)$": "<rootDir>/apps/api/src/$1",
            "^~apps/auth(|/.*)$": "<rootDir>/apps/auth/src/$1",
            "^~apps/search(|/.*)$": "<rootDir>/apps/search/src/$1",
            "^~apps/utility(|/.*)$": "<rootDir>/apps/utility/src/$1",
            "^~apps/managements(|/.*)$": "<rootDir>/apps/managements/src/$1",
            "^~apps/task(|/.*)$": "<rootDir>/apps/task/src/$1",
            "^~apps/order(|/.*)$": "<rootDir>/apps/order/src/$1",
            "^~apps/communications(|/.*)$": "<rootDir>/apps/communications/src/$1"
        }
    }
}
