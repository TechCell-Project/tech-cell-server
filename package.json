        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2",
        "webpack": "^5.89.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": [
            ".*\\.spec\\.ts$",
            ".*\\.test\\.ts$"
        ],
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/apps/",
            "<rootDir>/libs/"
        ],
        "moduleNameMapper": {
            "^~libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
            "^~libs/resource(|/.*)$": "<rootDir>/libs/resource/src/$1",
            "^~libs/third-party(|/.*)$": "<rootDir>/libs/third-party/src/$1",
            "^~apps/api(|/.*)$": "<rootDir>/apps/api/src/$1",
            "^~apps/auth(|/.*)$": "<rootDir>/apps/auth/src/$1",
            "^~apps/communications(|/.*)$": "<rootDir>/apps/communications/src/$1",
            "^~apps/managements(|/.*)$": "<rootDir>/apps/managements/src/$1",
            "^~apps/order(|/.*)$": "<rootDir>/apps/order/src/$1",
            "^~apps/search(|/.*)$": "<rootDir>/apps/search/src/$1",
            "^~apps/task(|/.*)$": "<rootDir>/apps/task/src/$1",
            "^~apps/utility(|/.*)$": "<rootDir>/apps/utility/src/$1"
        }
    },
    "lint-staged": {
        "linters": {
            "*.ts": [
                "compodoc --coverageTest"
            ]
        }
    },
    "packageManager": "yarn@4.1.0"
}
