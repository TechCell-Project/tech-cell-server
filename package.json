{
    "name": "tech-cell-server",
    "version": "0.0.1",
    "description": "",
    "author": "TechCell Teams <teams@techcell.cloud>",
    "private": true,
    "license": "MIT",
    "engines": {
        "node": "18.x"
    },
    "scripts": {
        "build": "nest build",
        "build:all": "node ./scripts/build-all-service.js",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node ./scripts/start-production.js",
        "lint": "eslint \"{src,apps,libs,test,types,constants}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./apps/back-end/test/jest-e2e.json",
        "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s",
        "tc-cli": "node ./scripts/deploy-docker.js",
        "up:core": "yarn tc-cli core --up",
        "down:core": "yarn tc-cli core --down",
        "restart:core": "yarn down:core && yarn up:core",
        "update:core": "yarn tc-cli core --pull && yarn up:core",
        "up:prod": "yarn tc-cli prod --up",
        "down:prod": "yarn tc-cli prod --down",
        "restart:prod": "yarn down:prod && yarn up:prod",
        "update:prod": "yarn tc-cli prod --pull && yarn up:prod",
        "get-logs": "docker cp swag:/config/log/. ./logs && docker cp utility:/usr/src/app/logs ."
    },
